        "tailwindcss": "^0.7.4",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.5.22"
    },
    "dependencies": {
        "eslint": "^5.12.1",
        "eslint-config-prettier": "^3.6.0",
        "eslint-plugin-vue": "^5.1.0",
        "laravel-mix-purgecss": "^4.1.0",
        "laravel-mix-tailwind": "^0.1.0",
        "prettier": "^1.16.1",
        "vform": "^1.0.0"
    }
